{
  "index_patterns": [
    "logstash-ecs-*"
  ],
  "order": 3,
  "mappings": {
    "_meta": {
      "version": "0.1.1-dev"
    },
    "dynamic_templates": [
      {
        "strings_as_keyword": {
          "mapping": {
            "ignore_above": 1024,
            "type": "keyword"
          },
          "match_mapping_type": "string"
        }
      }
    ],
    "date_detection": false,
    "properties": {
      "zeek": {
        "properties": {
          "dhcp": {
            "properties": {
              "uids": {
                "ignore_above": 1024,
                "type": "keyword"
              },
              "ip_requested": {
                "type": "ip"
              },
              "ip_assigned": {
                "type": "ip"
              },
              "lease_time": {
                "type": "double"
              },
              "client_message": {
                "ignore_above": 1024,
                "type": "keyword"
              },
              "server_message": {
                "ignore_above": 1024,
                "type": "keyword"
              },
              "msg_types": {
                "ignore_above": 1024,
                "type": "keyword"
              }
            }
          }
        }
      }
    }
  }
}
